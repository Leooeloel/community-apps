{
  "name": "@netless/<%- fullName %>",
  "version": "0.1.0",
  "main": "dist/main.cjs.js",
  "module": "dist/main.es.js",
  "scripts": {
    "start": "node ../../../scripts/start.js",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "esbuild src/index.js --bundle --format=cjs --outfile=dist/main.cjs.js",
    "build:esm": "esbuild src/index.js --bundle --format=esm --outfile=dist/main.es.js"
  }
}
